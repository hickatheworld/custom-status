{
  "name": "custom-status",
  "displayName": "Custom Status",
  "description": "Small VS Code extension to display a message of yours in the status bar.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.58.0"
  },
  "icon": "icon.png",
  "publisher": "hickatheworld",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hickatheworld/custom-status.git"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "custom-status.edit",
        "category": "Custom Status",
        "title": "Edit Custom Message"
      },
      {
        "command": "custom-status.switch",
        "category": "Custom Status",
        "title": "Switch Display"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vsce": "^1.96.1",
    "vscode-test": "^1.5.2"
  }
}
